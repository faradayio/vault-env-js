{
  "name": "vault-env",
  "version": "5.0.0-beta.2",
  "description": "Put your vault secrets in your process.env",
  "main": "main.js",
  "types": "main.d.ts",
  "files": [
    "*.d.ts",
    "*.js"
  ],
  "scripts": {
    "clean": "rm -f *.d.ts *.js test/*.d.ts test/index.js test/fakeVault*.js",
    "build": "tsc",
    "fmt": "prettier --write .",
    "lint": "eslint *.ts test/**/*.ts",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "mocha -r ts-node/register test/index.ts"
  },
  "bin": {
    "vault-env": "cli.js"
  },
  "keywords": [
    "vault",
    "env",
    "hashicorp"
  ],
  "author": "Madeline Davies <madeline@faraday.io>",
  "maintainers": [
    "Eric Kidd <eric.kidd@faraday.io>"
  ],
  "license": "MIT",
  "dependencies": {
    "find-root": "^1.0.0",
    "sync-request": "^6.1.0",
    "then-request": "^6.0.2"
  },
  "devDependencies": {
    "@types/find-root": "^1.1.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^11.0.1",
    "prettier": "^2.1.2",
    "tmp": "^0.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
